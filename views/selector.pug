extends layout

block content
  h1= title
  - var image_names = ["dog", "food", "music", "paris", "photograph"]
  - for(let i = 0; i < 3; i++) {
    - let number = Math.floor(Math.random() * 5)
        img(src=`/images/${image_names[number]}.jpg`, alt="alternate images other time ", height="300px", width="300px")
  - }